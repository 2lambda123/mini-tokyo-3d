    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-strip": "^2.0.1",
    "eslint": "^7.29.0",
    "eslint-config-mourner": "^3.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.1",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-import": "^14.0.2",
