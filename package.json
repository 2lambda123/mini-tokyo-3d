    "@turf/line-slice": "^5.1.5",
    "@turf/meta": "^5.1.5",
    "@turf/nearest-point-on-line": "^5.1.5",
    "@turf/polygon-to-line": "^5.1.5",
    "@turf/truncate": "^5.1.5",
    "@turf/union": "^5.1.5",
    "csscolorparser": "^1.0.3",
    "japanese-holidays": "^1.0.10",
    "loaders.css": "^0.1.2",
    "mapbox-gl": "^2.3.1",
