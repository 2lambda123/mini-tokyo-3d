    "rollup": "2.48.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "scripts": {
    "build": "rollup -c",
    "build-pages": "rm -rf build; cp -r public build; cp .npmignore secrets dist/mini-tokyo-3d.min.css dist/mini-tokyo-3d.min.js dist/mini-tokyo-3d.min.js.map build",
    "build-data": "rm -rf build/data; mkdir -p build/data; cp data/dictionary-*.json data/osm-liberty.json build/data; node dist/loader",
    "build-all": "run-s build build-pages build-data",
    "lint": "eslint --cache --ignore-path .gitignore src"
  },
  "files": [
    "build/",
    "dist/mini-tokyo-3d.*",
    "src/",
    "USER_GUIDE-*",
    "DEVELOPER_GUIDE-*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nagix/mini-tokyo-3d.git"
